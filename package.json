{
  "name": "pro_shop",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "engines": {
    "node": "16.x"
  },
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start",
    "dev": "npx concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node seeder",
    "data:destroy": "node seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aditiii23/pro_shop.git"
  },
  "author": " Aditi Maurya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aditiii23/pro_shop/issues"
  },
  "homepage": "https://github.com/aditiii23/pro_shop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.3"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.13"
  }
}
